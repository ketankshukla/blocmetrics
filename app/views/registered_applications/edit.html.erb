<div class="page-header">
  <h1>Edit Application</h1>
</div>

<%= form_for @registered_application do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :url, "URL" %>
      <%= f.text_field :url %>
    </div>
    <%= f.submit "Save", class: 'btn btn-success' %>
<% end %>